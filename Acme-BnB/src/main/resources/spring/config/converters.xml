<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd		
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.0.xsd		
	">

	<util:list id="converters">
		<bean class="converters.ActorToStringConverter" />
		<bean class="converters.AdministratorToStringConverter" />
		<bean class="converters.AuditorToStringConverter" />
		<bean class="converters.AuditToStringConverter" />
		<bean class="converters.BookToStringConverter" />
		<bean class="converters.CommentToStringConverter" />
		<bean class="converters.ExtraAttributeToStringConverter" />
		<bean class="converters.FeeToStringConverter" />
		<bean class="converters.FinderToStringConverter" />
		<bean class="converters.InvoiceToStringConverter" />
		<bean class="converters.LessorToStringConverter" />
		<bean class="converters.PropertyToStringConverter" />
		<bean class="converters.RelatedValueToStringConverter" />
		<bean class="converters.SocialIdentityToStringConverter" />
		<bean class="converters.TenantToStringConverter" />
		
		
		<bean class="converters.StringToActorConverter" />
		<bean class="converters.StringToAdministratorConverter" />
		<bean class="converters.StringToAuditorConverter" />
		<bean class="converters.StringToAuditConverter" />
		<bean class="converters.StringToBookConverter" />
		<bean class="converters.StringToCommentConverter" />
		<bean class="converters.StringToExtraAttributeConverter" />
		<bean class="converters.StringToFeeConverter" />
		<bean class="converters.StringToFinderConverter" />
		<bean class="converters.StringToInvoiceConverter" />
		<bean class="converters.StringToLessorConverter" />
		<bean class="converters.StringToPropertyConverter" />
		<bean class="converters.StringToRelatedValueConverter" />
		<bean class="converters.StringToSocialIdentityConverter" />
		<bean class="converters.StringToTenantConverter" />
	</util:list>
	
</beans>